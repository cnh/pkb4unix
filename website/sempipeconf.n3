@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix semp:     <http://www.andonyar.com/rec/2012/sempipe/voc#> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix people:   <http://u.andonyar.com/people/> .
@prefix recpub:   <http://www.andonyar.com/rec/2007-01/aspec/recpub#> .
@prefix foaf:     <http://xmlns.com/foaf/0.1/> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix :         <#> .

<.> a semp:Module ;
    semp:buildDir <file:///home/urs/p/andonyar-image/> ;

    semp:confGraph <file:///home/urs/p/andonyar/mediatypes.n3> ;
    semp:confGraph <file:///home/urs/p/andonyar/publish.n3> ;

    semp:dataGraph <blog.rdf> ;
    semp:dataGraph <lenses.n3> ;
    semp:update (
    """
        INSERT {
            GRAPH <http://www.andonyar.com/rec/2012/pkb/A> {
                <http://www.andonyar.com/rec/2012/pkb/> ?p ?o .
            }
        }
        WHERE {
            <http://www.andonyar.com/rec/2012/pkb/> ?p ?o .
        }
    """
    """
        INSERT {
            GRAPH <http://www.andonyar.com/rec/2012/pkb/A> {
                <http://u.andonyar.com/people/UrsHolzer> ?p ?o .
            }
        }
        WHERE {
            <http://u.andonyar.com/people/UrsHolzer> ?p ?o .
        }
    """
    ) .

<http://www.andonyar.com/rec/2012/pkb/> a recpub:RecPub ;
    recpub:aspec <http://www.andonyar.com/rec/2007-01/aspec/rec2> ;
    recpub:licenseCategory <http://u.andonyar.com/ageneralvoc/LicenseNonfree> ;
    dcterms:rightsHolder <http://u.andonyar.com/people/UrsHolzer> ;
    dcterms:creator <http://u.andonyar.com/people/UrsHolzer> ;
    dcterms:description "About my personal knowledge base"@en ;
    dcterms:subject <http://dbpedia.org/resource/Personal_knowledge_base> ;
    dcterms:subject <http://dbpedia.org/resource/Semantic_Web> ;

    dcterms:dateCopyrighted "2013-02-17"^^xsd:date ;
    dcterms:created "2012-12-24"^^xsd:date ;
    dcterms:modified "2013-02-18"^^xsd:date .

<http://u.andonyar.com/people/UrsHolzer> a foaf:Person ;
    foaf:name "Urs Holzer" .

<http://www.andonyar.com/rec/2012/pkb/A> a semp:Resource ;
    semp:representation [
        semp:content-type "application/rdf+xml" ;
        semp:buildCommand semp:Serialize
    ] .

<http://www.andonyar.com/rec/2012/pkb/> a semp:Resource ;
    semp:representation [
        semp:content-type "text/html" ;
        semp:language "en" ;
        semp:buildCommand semp:Raw ;
        semp:source <index.en.html>
    ] .

<http://www.andonyar.com/rec/2012/pkb/blog> a semp:Resource ;
    semp:representation [
        semp:content-type "application/rdf+xml" ;
        semp:buildCommand semp:Raw ;
        semp:source <blog.rdf>
    ] ;
    semp:representation [
        semp:content-type "text/html" ;
        semp:buildCommand semp:Render ;
        semp:transformation ( <file:///home/urs/p/sempipe/transforms/fresnelsort.xsl> <blogtohtml.xsl> <file:///home/urs/p/sempipe/transforms/xhtml5toxhtml1.xsl> )
    ] .

